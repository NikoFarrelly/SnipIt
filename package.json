{
  "name": "snipit",
  "version": "0.1.0",
  "description": "Browser plugin to snip unwanted DOM elements",
  "author": "farrelly@thefarrelly.com",
  "scripts": {
    "clean": "rm -rf .output",
    "typescript": "tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "postinstall": "wxt prepare",
    "format:check": "prettier --check \"**/*.{ts,tsx}\"",
    "format": "prettier --write \"**/*.{ts,tsx,css,html}\""
  },
  "dependencies": {
  },
  "devDependencies": {
    "prettier": "^3.8.1",
    "typescript": "^5.5.3",
    "wxt": "^0.20.6"
  }
}
